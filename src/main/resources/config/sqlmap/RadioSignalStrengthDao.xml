    <insert id="add" parameterType="com.ips.dto.RadioSignalStrength" >
        insert into RadioSignalStrength (AP1,AP2,AP3,AP4,AP5,AP6,AP7,AP8,AP9,AP10,AP11,AP12,AP13,AP14,AP15,Cin)
